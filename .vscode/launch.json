{
  "version": "0.2.0",
  "configurations": [
    
    {
      "name": "Launch Current File",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${file}",
      "env": {
        "MOCK_API": true
      },
    },
    {
      "useApiV1": false,
      "dlvLoadConfig": {
        "followPointers": true,
        "maxVariableRecurse": 1,
        "maxStringLen": 3000,
        "maxArrayValues": 100,
        "maxStructFields": -1
      },
      "name": "Che Operator",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/manager/main.go",
      "env": {
        "WATCH_NAMESPACE": "che"
      },
      "cwd": "${workspaceFolder}",
      "args": [
        "--defaults-path",
        "deploy/operator.yaml"
      ]
    }
  ]
}
