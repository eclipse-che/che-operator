{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "bashdb",
      "request": "launch",
      "name": "Bash-Debug (release-olm-files.sh)",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/olm/release-olm-files.sh",
      "args": [
        "7.13.2"
      ]
    },
    {
      "type": "bashdb",
      "request": "launch",
      "name": "Bash-Debug (simplest configuration)",
      "program": "${file}"
    },
    {
      "name": "Launch Current File",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "program": "${file}",
      "env": {
        "MOCK_API": true
      },
    },
    {
      "useApiV1": false,
      "dlvLoadConfig": {
        "followPointers": true,
        "maxVariableRecurse": 1,
        "maxStringLen": 3000,
        "maxArrayValues": 100,
        "maxStructFields": -1
      },
      "name": "Che Operator",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/manager/main.go",
      "env": {
        "WATCH_NAMESPACE": "che",
        "CHE_FLAVOR": "che",
        "CONSOLE_LINK_NAME": "che",
        "CONSOLE_LINK_DISPLAY_NAME": "Eclipse Che",
        "CONSOLE_LINK_SECTION": "Red Hat Applications",
        "CONSOLE_LINK_IMAGE": "/dashboard/assets/branding/loader.svg",
        "CHE_IDENTITY_SECRET": "che-identity-secret",
        "CHE_IDENTITY_POSTGRES_SECRET": "che-identity-postgres-secret",
        "CHE_POSTGRES_SECRET": "che-postgres-secret"
      },
      "cwd": "${workspaceFolder}",
      "args": [
        "--defaults-path",
        "deploy/operator.yaml"
      ]
    }
  ]
}
